<%- include("../partials/header.ejs", {title: "Game Of Thrones"}) %>


<p>"I drink and I know things" - Tyrion Lannister</p>

<section>
  <!--<p><a href="/db-test">Clicking this link will test to see if your db connection is ok</a></p>-->



  <div class="grid-container">
    <% fivePiecesArray.forEach((element, index) => { %>
        <div class="card">
        <p> <%= episodeCodes[index]%></p>
      <h3>Title: <%= element.name %></h3>
      <img src="<%= element.images.medium%>" alt="Episode Image">
      <p>Summary: <%- element.summary%></p>
    </div>
    <% })%>
  </div>
</section>

<%- include("../partials/footer.ejs") %>